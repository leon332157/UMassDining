{
  "name": "umassdining",
  "version": "0.0.1",
  "description": "umass dining api",
  "scripts": {
    "start:https": "parcel src/index.html -p 8443 --https --dist-dir out",
    "start": "parcel src/index.html -p 3000 --dist-dir out",
    "build": "parcel build src/index.html --no-optimize",
    "clean": "rimraf -rf dist out",
    "build:tsc": "tsc",
    "lint": "eslint src --ext .ts",
    "build:copy": "cpy src/*/* !src/*.ts out",
    "test": "echo \"Error: no test specified\" && exit 1",
    "publish": "wrangler pages publish dist",
    "build:publish": "npm run build && npm run publish"
  },
  "targets": {
    "modern": {
      "outputFormat": "global"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leon332157/UMassDining.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leon332157/UMassDining/issues"
  },
  "homepage": "https://github.com/leon332157/UMassDining#readme",
  "devDependencies": {
    "@azure/msal-browser": "^2.37.0",
    "@parcel/config-default": "^2.8.3",
    "@parcel/packager-raw-url": "^2.8.3",
    "@parcel/transformer-typescript-tsc": "^2.8.3",
    "@parcel/transformer-webmanifest": "^2.8.3",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@typescript-eslint/parser": "^5.59.2",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.39.0",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "parcel": "^2.8.3",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "wrangler": "^2.19.0"
  },
  "type": "module"
}
